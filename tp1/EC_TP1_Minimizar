y"}},{"cell_type":"code","source":["def Energ(Vo, p):\n","    L = (p+2)*0.134  # well width in nm\n","\n","    # Universal constants\n","    hbar = 6.582119569e-16 # eV * s\n","    c = 299792458 # m/s\n","    m_e = 510.998e3 / c**2 # eV / c^2\n","\n","    theta0 = np.sqrt(2 * m_e * Vo) * (L*1e-9) / (2 * hbar)\n","\n","    # define our difference equations for the even and odd solutions\n","    def even_sol(x):\n","        y1 = tan(x)\n","        y = sqrt((theta0/x)**2 - 1)\n","        return y1-y # (y - y1)\n","\n","    def odd_sol(x):\n","        y2 = -1/tan(x)\n","        y = sqrt((theta0/x)**2 - 1)\n","        return y2-y #(y - y2)\n","\n","    def sqWellSol(theta0, step):\n","        esol = array([], dtype=float)  # array to store even solutions\n","        osol = array([], dtype=float)  # array to store odd solutions\n","        inc = arange(step, theta0, step)  # array of \"x\"s to find solutionsd\n","\n","        even = True  # alternate even/odd\n","        for i in range(len(inc)-1):\n","            if even:\n","                if even_sol(inc[i]) * even_sol(inc[i+1]) < 0:\n","                    esol = append(esol, optimize.brentq(even_sol, inc[i], inc[i+1]))\n","                    even = False\n","            else:\n","                if odd_sol(inc[i]) * odd_sol(inc[i+1]) < 0:\n","                    osol = append(osol, optimize.brentq(odd_sol, inc[i], inc[i+1]))\n","                    even = True\n","\n","        return esol, osol\n","\n","    etheta, otheta = sqWellSol(theta0, 0.001)\n","\n","    # Converting theta solutions into energies\n","    ek = etheta*2./L\n","    ok = otheta*2/L\n","    ekap = 2*etheta/L*tan(etheta)\n","    okap = abs(2*otheta/L*tan(otheta))\n","\n","    def e_from_theta(theta):\n","      return 2 * hbar**2 * theta**2 / (m_e * (L * 1e-9)**2)\n","\n","    e_eng = e_from_theta(etheta)\n","    o_eng = e_from_theta(otheta)\n","    E_all = sorted(list(np.concatenate([e_eng, o_eng])))\n","\n","    return E_all\n"],"metadata":{"id":